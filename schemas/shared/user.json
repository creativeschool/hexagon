{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://hex.zhangzisu.cn/user.json",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string"
    },
    "login": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "created": {
      "type": "number"
    },
    "updated": {
      "type": "number"
    }
  }
}