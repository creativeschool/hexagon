{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://hex.zhangzisu.cn/file.json",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string"
    },
    "course": {
      "type": "string"
    },
    "path": {
      "type": "string"
    },
    "versions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "hash": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        }
      }
    },
    "tags": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      }
    },
    "created": {
      "type": "number"
    },
    "updated": {
      "type": "number"
    }
  }
}